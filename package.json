{
  "name": "convex-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --raw \"bun run dev:server\" \"bun run dev:convex\"",
    "dev:server": "NODE_ENV=development bun run --watch server/index.ts",
    "dev:convex": "bun run convex dev",
    "build": "bun run vite build --mode production && esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist --external:lightningcss --external:@tailwindcss/oxide* --external:@babel/* --external:convex --external:nanoid --external:vite",
    "start": "NODE_ENV=production node dist/index.js",
    "typecheck": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },

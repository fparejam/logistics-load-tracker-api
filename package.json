{
  "name": "convex-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --raw \"bun run dev:server\" \"bun run dev:convex\"",
    "dev:server": "NODE_ENV=development bun run --watch server/index.ts",
    "dev:convex": "bun run convex dev",
    "build": "bun run vite build --mode production && esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist --external:lightningcss --external:@tailwindcss/oxide* --external:@babel/* --external:convex --external:nanoid --external:vite",
    "start": "NODE_ENV=production node dist/index.js",
    "typecheck": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ag-charts-react": "^12.3.1",
    "ag-charts-enterprise": "^12.3.1",
    "mapbox-gl": "^3.16.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "color": "^4.2.3",
    "convex": "^1.25.0",
    "convex-helpers": "^0.1.101",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.3.0",
    "express": "^4.18.2",
    "lightningcss": "^1.30.1",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.536.0",
    "motion": "^11.15.0",
    "nanoid": "^3.3.11",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
